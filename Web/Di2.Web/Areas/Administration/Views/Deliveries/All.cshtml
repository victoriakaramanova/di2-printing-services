@using Di2.Web.ViewModels.Categories.ViewModels
@model IEnumerable<CategoryProductsViewModel>

@using Di2.Common
@{
    this.ViewData["Title"] = "List All Deliveries";
}

@foreach (var item in Model)
{
    <div class="row">
        @item.MaterialName
        @item.Description
        @item.SubCategoryName
        @item.Quantity
        @item.AvgPrice
        @item.Cost
        @item.UnitPrice
    </div>
}
