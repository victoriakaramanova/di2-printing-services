@using Di2.Web.ViewModels.Deliveries //.
@model IEnumerable<DeliveryViewModel>

    @using Di2.Common
    @{
        this.ViewData["Title"] = "List All Deliveries";
    }

    @foreach (var item in Model)
    {
        @item.MaterialName
        @item.MaterialDescription
        @item.Quantity
        @item.UnitPrice
        <img src="@item.MaterialImage" />
    }
