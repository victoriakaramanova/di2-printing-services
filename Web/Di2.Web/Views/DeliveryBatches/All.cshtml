@using Di2.Web.ViewModels.DeliveryBatches
@model IEnumerable<AllDeliveryBatchesViewModel>

@using Di2.Common
@{
    this.ViewData["Title"] = "List All Deliveries";
}

@foreach (var item in Model)
{
    <h3>@item.MaterialName</h3>
    <h3>@item.Color</h3>
    <h3>@item.ReferenceKey</h3>
    <h3>@item.Quantity</h3>
    <h3>@item.Cost</h3>
    <h3>@item.SupplierName</h3>
}

