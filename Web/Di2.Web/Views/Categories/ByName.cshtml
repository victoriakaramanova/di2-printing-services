@model Di2.Web.ViewModels.Categories.ViewModels.CategoryProductsViewModel
@{ 
    this.ViewData["Title"] = "Продукти";
}

@foreach (var product in Model.Deliveries)
{
    <div class="row">
        <a>@product.Name</a>
        @product.Description
        @product.ExtraInfo
        @product.AvgPrice.ToString("#.##")
        @product.SubCategoryName
        @product.Quantity
       <a><img src="@product.Image" /></a>
    </div>
}
