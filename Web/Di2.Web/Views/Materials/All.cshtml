@using Di2.Web.ViewModels.Materials.ViewModels
@model IEnumerable<MaterialsViewModel>

@using Di2.Common
@{
    this.ViewData["Title"] = "List All Materials";
}

@foreach (var item in Model)
{
    <h3>@item.Name</h3>
    <h4>@item.Description</h4>
    <h4>@item.ExtraInfo</h4>
   
    <h4>@item.SubCategory</h4>
}

